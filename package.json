{
  "name": "rusty-butter-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:detached": "nohup next dev > dev.log 2>&1 & echo 'Dev server started in background. Check dev.log for output.'",
    "dev:stop": "pkill -f 'next dev' && echo 'Dev server stopped'",
    "dev:logs": "tail -f dev.log",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "lucide-react": "^0.535.0",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5.8.3"
  }
}
